{
  "name": "react-cm-ui",
  "version": "0.14.0-beta-29",
  "description": "React UI for Church management",
  "main": "lib/",
  "jsnext:main": "src/",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saddlebackdev/react-cm-ui.git"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/saddlebackdev/react-cm-ui/issues"
  },
  "keywords": [
    "button",
    "drawer",
    "grid",
    "icon",
    "label",
    "responsive",
    "react",
    "react-component",
    "church management",
    "cm"
  ],
  "homepage": "https://github.com/saddlebackdev/react-cm-ui#readme",
  "scripts": {
    "build": "npm run docs:prod",
    "clean:lib": "rimraf lib",
    "compile": "npm run clean:lib && node-sass src/scss/Style.scss -o lib/css --output-style compressed && cp -r src/fonts lib/fonts && babel src/ -d lib/",
    "docs:dev": "webpack-dev-server --content-base docs/build/ --hot --progress --colors --host 0.0.0.0",
    "docs:prod": "webpack",
    "docs:stats": "webpack --display-modules --display-chunks --json > stats.json --progress",
    "postpublish": "git checkout dev && git push && git push --tags",
    "prepublish": "npm run compile",
    "publish": "echo 'Published $npm_package_version'",
    "release:finish": "git flow release finish v$npm_package_version && git push && git push --tags && git checkout master && git push",
    "release:start": "git flow release start v$npm_package_version && git push --set-upstream origin release/v$npm_package_version",
    "start": "npm run docs:dev"
  },
  "dependencies": {
    "autosize": "^3.0.17",
    "classnames": "^2.2.5",
    "lodash": "^4.15.0",
    "moment-timezone": "^0.5.13",
    "react": "^0.14.8",
    "react-custom-scrollbars": "^4.0.0",
    "react-dom": "^0.14.8",
    "react-onclickoutside": "^5.7.1",
    "react-portal": "^3.0.0",
    "react-responsive": "^1.2.7",
    "react-router": "^3.0.0",
    "react-select": "^1.0.0-rc.3",
    "react-syntax-highlighter": "^4.0.1",
    "react-tether": "^0.3.3",
    "react-text-mask": "^0.19.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.4.0",
    "extract-text-webpack-plugin": "2.0.0-rc.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.4.0",
    "postcss-loader": "^1.2.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
